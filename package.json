{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "React TODO App",
  "main": "index.js",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "start": "http-server build",
    "browser-sync": "browser-sync start --config bs-config.js --color",
    "build": "webpack",
    "watch": "concurrently \"webpack --watch --color\" \"npm run browser-sync\"",
    "pretest": "npm run lint",
    "test": "mocha --compilers jsx:babel-core/register test",
    "test:e2e": "wdio wdio.conf.js",
    "lint": "eslint .",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "heroku-postbuild": "npm run build -- -p"
  },
  "keywords": [],
  "author": "ybiquitous <10koba01@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "http-server": "^0.9.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "wdio-mocha-framework": "^0.4.0",
    "webdriver-manager": "^10.2.3",
    "webdriverio": "^4.2.8",
    "webpack": "^1.13.2"
  }
}
